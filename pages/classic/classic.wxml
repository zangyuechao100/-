<view class="container">
  <view class="header">
    <v-epsoide class="epsoide" index="{{classicData.index}}"></v-epsoide>
    <v-like class="like" bind:like="onLike" count="{{classicData.fav_nums}}" like="{{classicData.like_status}}"></v-like>
  </view>
  <v-movie img="{{classicData.image}}" content="{{classicData.content}}"></v-movie>
  <v-nav bind:left="onNext" bind:right="onPrevious" class="nav" first="{{first}}" lastest="{{lastest}}" title="{{classicData.title}}"></v-nav>
</view>